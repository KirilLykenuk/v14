#include <stdio.h>

int main() {
    int q, w;
    int e, r, t;
    
    printf("Введіть перше ціле число: ");
    scanf("%d", &q);
    
    printf("Введіть друге ціле число: ");
    scanf("%d", &w);
    
    e = q % w;
    
    r = w - q;
    
    t = (q * q) + (w * w);
    
    printf("Залишок від ділення першого числа на друге: %d\n", e);
    printf("Різниця другого і першого числа: %d\n", r);
    printf("Сума квадратів заданих чисел: %d\n", t);
    
    return 0;
}
